<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vicki.mes.todo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/todo_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/todo_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        android:name="com.orm.SugarApp" >  <!--Use this attribute verbatim-->
        <meta-data
            android:name="DATABASE"
            android:value="bucketlist.db" />
        <meta-data
            android:name="VERSION"
            android:value="2" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.vicki.mes.todo.Models" />
        <activity android:name=".ui.ToDOActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.AddToDoActivity">
            
        </activity>
    </application>

</manifest>